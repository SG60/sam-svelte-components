{
	"name": "@samsveltecomponents/core",
	"version": "1.4.0",
	"repository": {
		"type": "git",
		"url": "git://github.com/sg60/sam-svelte-components.git",
		"directory": "packages/core"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "svelte-kit sync && svelte-package -o package",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --write --plugin-search-dir=. ."
	},
	"peerDependencies": {
		"svelte": "^3.44.0"
	},
	"devDependencies": {
		"@sveltejs/adapter-static": "1.0.6",
		"@sveltejs/kit": "1.30.4",
		"@sveltejs/package": "2.0.2",
		"@typescript-eslint/eslint-plugin": "5.46.1",
		"@typescript-eslint/parser": "5.46.1",
		"eslint": "8.43.0",
		"eslint-config-prettier": "8.8.0",
		"eslint-plugin-svelte3": "4.0.0",
		"prettier": "2.8.8",
		"prettier-plugin-svelte": "2.9.0",
		"svelte": "3.55.0",
		"svelte-check": "3.8.4",
		"svelte-loader": "3.2.3",
		"svelte-preprocess": "5.0.4",
		"svelte2tsx": "0.5.22",
		"tslib": "2.4.1",
		"typescript": "5.5.4",
		"vite": "4.5.3"
	},
	"type": "module",
	"publishConfig": {},
	"exports": {
		"./package.json": "./package.json",
		"./Icons/SvgIcon.svelte": {
			"types": "./package/Icons/SvgIcon.svelte.d.ts",
			"svelte": "./package/Icons/SvgIcon.svelte",
			"default": "./package/Icons/SvgIcon.svelte"
		},
		"./Icons/icons": {
			"types": "./package/Icons/icons.d.ts",
			"default": "./package/Icons/icons.js"
		},
		"./Modal.svelte": {
			"types": "./package/Modal.svelte.d.ts",
			"svelte": "./package/Modal.svelte",
			"default": "./package/Modal.svelte"
		},
		"./SoundCloud.svelte": {
			"types": "./package/SoundCloud.svelte.d.ts",
			"svelte": "./package/SoundCloud.svelte",
			"default": "./package/SoundCloud.svelte"
		},
		"./YouTube.svelte": {
			"types": "./package/YouTube.svelte.d.ts",
			"svelte": "./package/YouTube.svelte",
			"default": "./package/YouTube.svelte"
		}
	},
	"files": [
		"package"
	],
	"typesVersions": {
		">4.0": {
			"Icons/SvgIcon.svelte": [
				"./package/Icons/SvgIcon.svelte.d.ts"
			],
			"Icons/icons": [
				"./package/Icons/icons.d.ts"
			],
			"Modal.svelte": [
				"./package/Modal.svelte.d.ts"
			],
			"SoundCloud.svelte": [
				"./package/SoundCloud.svelte.d.ts"
			],
			"YouTube.svelte": [
				"./package/YouTube.svelte.d.ts"
			]
		}
	}
}
